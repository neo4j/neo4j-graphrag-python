{
    "version_": "1",
    "neo4j_config": {
        "uri": {
            "resolver_": "ENV",
            "var_": "NEO4J_URI"
        },
        "user": {
            "resolver_": "ENV",
            "var_": "NEO4J_USER"
        },
        "password": {
            "resolver_": "ENV",
            "var_": "NEO4J_PASSWORD"
        },
        "database": {
            "resolver_": "ENV",
            "var_": "NEO4J_DATABASE"
        }
    },
    "llm_config": {
        "name_": "openai",
        "class_": "OpenAILLM",
        "params_": {
            "api_key": {
                "resolver_": "ENV",
                "var_": "OPENAI_API_KEY"
            },
            "model_name": "gpt-4o"
        }
    },
    "embedder_config": {
        "name_": "openai",
        "class_": "OpenAIEmbeddings",
        "params_": {
            "api_key": {
                "resolver_": "ENV",
                "var_": "OPENAI_API_KEY"
            }
        }
    },
    "from_pdf": false,
    "entities": ["Person", {"label":  "Organization"}],
    "relations": ["WORKS_FOR", {"label":  "DIRECTED_BY"}],
    "potential_schema": [
        ["Person", "WORKS_FOR", "Organization"],
        ["Organization", "DIRECTED_BY", "Person"]
    ],
    "components": [
        {
            "_name": "text_splitter",
            "_class": "text_splitters.fixed_size_splitter.FixedSizeSplitter",
            "_params": {
                "chunk_size":  100,
                "chunk_overlap": 10
            }
        }
    ],
    "perform_entity_resolution": false
}
